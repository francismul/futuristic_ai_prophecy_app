{
  "version": "1.0.4",
  "name": "Oracle Mode - AI Prophecy Generator",
  "codename": "Version Nexus",
  "build": {
    "date": "2025-11-15",
    "environment": "production"
  },
  "features": {
    "pwa": true,
    "offline": true,
    "notifications": true,
    "backgroundSync": true,
    "scrollSnap": true,
    "sciFiInterface": true
  },
  "dependencies": {
    "threejs": "r128",
    "gsap": "3.12.2",
    "tailwindcss": "latest"
  },
  "cache": {
    "strategy": "cache-first",
    "maxAge": 604800,
    "autoCleanup": true
  },
  "compatibility": {
    "minBrowsers": {
      "chrome": "60",
      "firefox": "55",
      "safari": "11",
      "edge": "79"
    },
    "features": [
      "serviceWorker",
      "webManifest",
      "es6",
      "fetch",
      "promises"
    ]
  }
}